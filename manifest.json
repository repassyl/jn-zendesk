{
    "name": "Játéknet app",
    "author": {
        "name": "Laszlo Repassy",
        "email": "repassy.laszlo@jateknet.hu",
        "url": "https://www.jateknet.hu"
    },
    "defaultLocale": "en",
    "private": false,
    "location": {
        "support": {
            "ticket_sidebar": "assets/iframe.html"
        }
    },
    "version": "1.2",
    "frameworkVersion": "2.0",
    "parameters": [ {
            "name": "jateknet_api_root",
            "type": "url",
            "required": true,
            "secure": false,
            "default": "https://jateknet.hu/api/v1/"
        },
        {
            "name": "jateknet_api_token",
            "type": "text",
            "required": true,
            "secure": false,
            "default": ""
        },
        {
            "name": "jateknet_order_edit",
            "type": "url",
            "required": true,
            "secure": false,
            "default": ""
        },
        {
            "name": "debug",
            "type": "checkbox",
            "required": false,
            "secure": false,
            "default": false
        },
        {
            "name": "order_id_ticket_field_name",
            "type": "text",
            "required": true,
            "secure": false,
            "default": "ticket.customField:custom_field_QQQQQQQQQQQQ"
        }
    ]
}
